@model Intex2ABBCAuthentication.Models.ViewModels.CrashFilter

<form asp-action="SummaryData" method="post">
    <label asp-for="year">Year</label>
    <input asp-for="year" />

    <label asp-for="month">Month</label>
    <input asp-for="month" />

    <label asp-for="city">City</label>
    <input asp-for="city" />

    <label asp-for="county">County</label>
    <input asp-for="county" />

    <label asp-for="severity">Severity</label>
    <input asp-for="severity" />

    <div class="float-end">
        <input class="btn btn-primary" type="submit" value="Filter Results" />
    </div>

</form>

